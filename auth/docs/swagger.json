{
    "swagger": "2.0",
    "info": {
        "description": "Authentication service for the Dinners app",
        "title": "Dinners Auth",
        "contact": {
            "name": "API Support",
            "url": "http://deepdi.sh/support",
            "email": "me@zed.gay"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "0.0.1"
    },
    "host": "localhost:8080",
    "paths": {
        "/": {
            "post": {
                "description": "Register a new account",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "accounts"
                ],
                "summary": "Register",
                "parameters": [
                    {
                        "description": "Registration request body",
                        "name": "registration_request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/http.RegistrationRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/github_com_zednotdead_dinners_auth_internal_domain_models.User"
                        }
                    }
                }
            }
        },
        "/login": {
            "post": {
                "description": "Log in to the account",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "accounts"
                ],
                "summary": "Log in",
                "parameters": [
                    {
                        "description": "Login request object",
                        "name": "login_request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/http.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/github_com_zednotdead_dinners_auth_internal_domain_models.User"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "github_com_zednotdead_dinners_auth_internal_domain_models.Credential": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "passwordHash": {
                    "type": "string"
                },
                "userID": {
                    "type": "string"
                }
            }
        },
        "github_com_zednotdead_dinners_auth_internal_domain_models.User": {
            "type": "object",
            "properties": {
                "avatar": {
                    "type": "string"
                },
                "credentials": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/github_com_zednotdead_dinners_auth_internal_domain_models.Credential"
                    }
                },
                "email": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "http.LoginRequest": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string",
                    "minLength": 5,
                    "example": "securepassword"
                },
                "username": {
                    "type": "string",
                    "minLength": 5,
                    "example": "username"
                }
            }
        },
        "http.RegistrationRequest": {
            "type": "object",
            "required": [
                "email",
                "password",
                "username"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "example": "username@example.com"
                },
                "password": {
                    "type": "string",
                    "minLength": 5,
                    "example": "securepassword"
                },
                "username": {
                    "type": "string",
                    "minLength": 5,
                    "example": "username"
                }
            }
        }
    },
    "externalDocs": {
        "description": "OpenAPI",
        "url": "https://swagger.io/resources/open-api/"
    }
}